<div class="right">
	

	<div class="categories">
		<div class="categories_title">
		
			<h2>Categories</h2>
			
		</div>
		<div class="categories_content">
			<ul>
				<% categ.forEach(function(categ,index){ %>
					<li><a href="/category/<%= categ.name%>"><%= categ.name  %> 
					<sup><%= categ.nums%></sup>

						 
					</a></li>
				<% })%>
			</ul>
		</div>

	</div>

	<div class="archives">
		<div class="archives_title">
			<h2>Archives</h2>
		</div>
		<div class="archives_content">
			<ul>
				<% cat.forEach(function(cat,index){ %>
					<li><a href="/archive/<%= cat.time%>"><%= cat.time  %> 
					<sup><%= cat.nums%></sup>
						 
					</a></li>
				<% })%>
			</ul>
			
		</div>
	</div>

	<div class="tags">
		
	</div>
</div><!-- right 结束 -->